{
    "return": true,
    "metric": {
        "name": "error",
        "minimize": true,
        "num_trials": 1
    },
    "learning_rate": {
        "flag": true,
        "param": {
            "name": "learning_rate",
            "type": "range",
            "bounds": [
                1e-05,
                0.5
            ],
            "log_scale": true
        },
        "default": "slice(None, 0.003, None)"
    },
    "num_epochs": {
        "flag": false,
        "param": {
            "name": "num_epochs",
            "type": "range",
            "bounds": [
                2,
                50
            ],
            "digits": 0
        },
        "default": 0
    },
    "momentum0": {
        "flag": true,
        "param": {
            "name": "momentum0",
            "type": "range",
            "bounds": [
                0.9,
                0.99
            ]
        },
        "default": 0.95
    },
    "momentum1": {
        "flag": true,
        "param": {
            "name": "momentum1",
            "type": "range",
            "bounds": [
                0.8,
                0.89
            ]
        },
        "default": 0.85
    },
    "dropout_ps": {
        "flag": true,
        "param": {
            "name": "dropout_ps",
            "type": "range",
            "bounds": [
                0,
                1
            ]
        },
        "default": null
    },
    "use_bn": {
        "flag": false,
        "param": {
            "name": "use_bn",
            "type": "choice",
            "values": [
                true,
                false
            ]
        },
        "default": true
    }
}